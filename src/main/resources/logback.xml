<configuration>
    <!-- Console Appender -->
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd'T'HH:mm:ss.SSS} [%thread] %-5level %class - %msg%n</pattern>
        </encoder>
    </appender>

    <!-- Root Logger -->
    <root level="debug">
        <appender-ref ref="CONSOLE" />
    </root>

    <!-- Specific Logger -->
    <!-- <logger name="org.apache.activemq" level="trace" />
    <logger name="org.messaginghub.pooled" level ="trace"/> -->
    <logger name="javax.net.debug" level="debug"/>
</configuration>
